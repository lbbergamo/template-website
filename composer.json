{
  "name": "template/website",
  "description": "Template for website with routes and minify",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Leandro_Bergamo",
      "email": "leandro@leandrobergamo.dev",
      "homepage": "http://leandrobergamo.dev",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "vendor"
  },
  "autoload": {
    "psr-4": {
      "Source\\": "Source/"
    },
    "files": [
      "Source/Boot/Config.php",
      "Source/Boot/Helpers.php",
      "Source/Boot/Minify.php"
    ]
  },
  "require": {
    "php": "^7.1",
    "ext-pdo": "^7.1",
    "phpmailer/phpmailer": "~6.0",
    "league/plates": "v4.0.0-alpha",
    "matthiasmullie/minify": "^1.3",
    "coffeecode/optimizer": "^1.0",
    "coffeecode/router": "^1.0"
  }
}